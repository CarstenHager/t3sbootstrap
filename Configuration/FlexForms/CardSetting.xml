<T3DataStructure>
	<meta>
		<langDisable>1</langDisable>
	</meta>
	<sheets>
		<sDEF>
			<ROOT>
				<sheetTitle>Card Settings</sheetTitle>
				<type>array</type>
				<el>
					<effect>
						<label>Card Hover Effect</label>
						<config>
							<type>select</type>
							<renderType>selectSingle</renderType>
							<items type="array">
								<numIndex index="0" type="array">
									<numIndex index="label">None</numIndex>
									<numIndex index="value">0</numIndex>
								</numIndex>
								<numIndex index="1" type="array">
									<numIndex index="label">zoom and border</numIndex>
									<numIndex index="value">one</numIndex>
								</numIndex>
							</items>
							<default>0</default>
						</config>
					</effect>
					<maxwidth>
						<label>Card max-width == image width</label>
						<displayCond>USER:T3SBS\T3sbootstrap\UserFunction\TcaMatcher->cardWrapperParent</displayCond>
						<config>
							<type>check</type>
						</config>
					</maxwidth>
					<image.overlay>
						<label>LLL:EXT:t3sbootstrap/Resources/Private/Language/locallang_be.xlf:flexforms.card.imageOverlay</label>
						<displayCond>
							<and>
								<numIndex index="0">USER:T3SBS\T3sbootstrap\UserFunction\TcaMatcher->cardWrapperParent</numIndex>
								<numIndex index="1">FIELD:sDEF.multiImage.enable:=:0</numIndex>
							</and>
						</displayCond>
						<config>
							<type>check</type>
						</config>
					</image.overlay>
					<mobile.overlay>
						<label>Image overlay - mobile view only</label>
						<displayCond>
							<and>
								<numIndex index="0">USER:T3SBS\T3sbootstrap\UserFunction\TcaMatcher->cardWrapperParent</numIndex>
								<numIndex index="1">FIELD:sDEF.multiImage.enable:=:0</numIndex>
							</and>
						</displayCond>
						<config>
							<type>check</type>
						</config>
					</mobile.overlay>
					<flipcard>
						<label>Flip Card with Text or Image</label>
						<description>Frontside with image - Backside with text (if "Text Top" available) or image (second image)</description>
						<displayCond>
							<and>
								<numIndex index="0">USER:T3SBS\T3sbootstrap\UserFunction\TcaMatcher->cardWrapperParent</numIndex>
								<numIndex index="1">FIELD:sDEF.multiImage.enable:=:0</numIndex>
							</and>
						</displayCond>
						<onChange>reload</onChange>
						<config>
							<type>check</type>
						</config>
					</flipcard>
					<rotateY>
						<label>Number of rotations in degree</label>
						<displayCond>FIELD:flipcard:=:1</displayCond>
						<config>
							<type>select</type>
							<renderType>selectSingle</renderType>
							<items type="array">
								<numIndex index="0" type="array">
									<numIndex index="label">180deg</numIndex>
									<numIndex index="value">180deg</numIndex>
								</numIndex>
								<numIndex index="1" type="array">
									<numIndex index="label">540deg</numIndex>
									<numIndex index="value">540deg</numIndex>
								</numIndex>
								<numIndex index="2" type="array">
									<numIndex index="label">900deg</numIndex>
									<numIndex index="value">900deg</numIndex>
								</numIndex>
							</items>
							<default>180deg</default>
						</config>
					</rotateY>
					<title.onTop>
						<label>LLL:EXT:t3sbootstrap/Resources/Private/Language/locallang_be.xlf:flexforms.card.titleOnTop</label>
						<config>
							<type>check</type>
						</config>
					</title.onTop>
					<button.enable>
						<onChange>reload</onChange>
						<label>Enable button with header_link</label>
						<config>
							<type>check</type>
						</config>
					</button.enable>
					<button.stretchedLink>
						<label>Stretched link (link the entire Card)</label>
						<displayCond>FIELD:button.enable:=:1</displayCond>
						<config>
							<type>check</type>
						</config>
					</button.stretchedLink>
					<button.style>
						<label>Predefined button styles</label>
						<displayCond>FIELD:button.enable:=:1</displayCond>
						<config>
							<type>select</type>
							<renderType>selectSingle</renderType>
							<items type="array">
								<numIndex index="0" type="array">
									<numIndex index="label">Secondary</numIndex>
									<numIndex index="value">secondary</numIndex>
								</numIndex>
								<numIndex index="1" type="array">
									<numIndex index="label">LLL:EXT:t3sbootstrap/Resources/Private/Language/locallang_be.xlf:primary</numIndex>
									<numIndex index="value">primary</numIndex>
								</numIndex>
								<numIndex index="2" type="array">
									<numIndex index="label">LLL:EXT:t3sbootstrap/Resources/Private/Language/locallang_be.xlf:success</numIndex>
									<numIndex index="value">success</numIndex>
								</numIndex>
								<numIndex index="3" type="array">
									<numIndex index="label">LLL:EXT:t3sbootstrap/Resources/Private/Language/locallang_be.xlf:info</numIndex>
									<numIndex index="value">info</numIndex>
								</numIndex>
								<numIndex index="4" type="array">
									<numIndex index="label">LLL:EXT:t3sbootstrap/Resources/Private/Language/locallang_be.xlf:warning</numIndex>
									<numIndex index="value">warning</numIndex>
								</numIndex>
								<numIndex index="5" type="array">
									<numIndex index="label">LLL:EXT:t3sbootstrap/Resources/Private/Language/locallang_be.xlf:danger</numIndex>
									<numIndex index="value">danger</numIndex>
								</numIndex>
								<numIndex index="6" type="array">
									<numIndex index="label">Link</numIndex>
									<numIndex index="value">card-link</numIndex>
								</numIndex>
							</items>
							<default>secondary</default>
						</config>
					</button.style>
					<button.text>
						<label>LLL:EXT:t3sbootstrap/Resources/Private/Language/locallang_be.xlf:flexforms.card.buttonText</label>
						<displayCond>FIELD:button.enable:=:1</displayCond>
						<config>
							<type>input</type>
							<checkbox></checkbox>
							<eval>trim</eval>
							<size>48</size>
							<max>99</max>
							<default></default>
						</config>
					</button.text>
					<button.outline>
						<label>Button Outline</label>
						<displayCond>FIELD:button.enable:=:1</displayCond>
						<config>
							<type>check</type>
						</config>
					</button.outline>
					<button.footer>
						<label>Button in Card Footer</label>
						<displayCond>FIELD:button.enable:=:1</displayCond>
						<config>
							<type>check</type>
						</config>
					</button.footer>
					<button.block>
						<label>Block level button</label>
						<displayCond>FIELD:button.enable:=:1</displayCond>
						<config>
							<type>check</type>
						</config>
					</button.block>
					<cardborder>
						<label>Custom Border</label>
						<onChange>reload</onChange>
						<config>
							<type>check</type>
						</config>
					</cardborder>
					<cardborderstyle>
						<label>Predefined border styles</label>
						<displayCond>FIELD:sDEF.cardborder:=:1</displayCond>
						<config>
							<type>select</type>
							<renderType>selectSingle</renderType>
							<items type="array">
								<numIndex index="0" type="array">
									<numIndex index="label">LLL:EXT:t3sbootstrap/Resources/Private/Language/locallang_be.xlf:secondary</numIndex>
									<numIndex index="value">secondary</numIndex>
								</numIndex>
								<numIndex index="1" type="array">
									<numIndex index="label">LLL:EXT:t3sbootstrap/Resources/Private/Language/locallang_be.xlf:primary</numIndex>
									<numIndex index="value">primary</numIndex>
								</numIndex>
								<numIndex index="2" type="array">
									<numIndex index="label">LLL:EXT:t3sbootstrap/Resources/Private/Language/locallang_be.xlf:success</numIndex>
									<numIndex index="value">success</numIndex>
								</numIndex>
								<numIndex index="3" type="array">
									<numIndex index="label">LLL:EXT:t3sbootstrap/Resources/Private/Language/locallang_be.xlf:info</numIndex>
									<numIndex index="value">info</numIndex>
								</numIndex>
								<numIndex index="4" type="array">
									<numIndex index="label">LLL:EXT:t3sbootstrap/Resources/Private/Language/locallang_be.xlf:warning</numIndex>
									<numIndex index="value">warning</numIndex>
								</numIndex>
								<numIndex index="5" type="array">
									<numIndex index="label">LLL:EXT:t3sbootstrap/Resources/Private/Language/locallang_be.xlf:danger</numIndex>
									<numIndex index="value">danger</numIndex>
								</numIndex>
								<numIndex index="6" type="array">
									<numIndex index="label">light</numIndex>
									<numIndex index="value">light</numIndex>
								</numIndex>
								<numIndex index="7" type="array">
									<numIndex index="label">dark</numIndex>
									<numIndex index="value">dark</numIndex>
								</numIndex>
								<numIndex index="8" type="array">
									<numIndex index="label">white</numIndex>
									<numIndex index="value">white</numIndex>
								</numIndex>
								<numIndex index="9" type="array">
									<numIndex index="label">none</numIndex>
									<numIndex index="value">0</numIndex>
								</numIndex>
							</items>
							<default>secondary</default>
						</config>
					</cardborderstyle>
					<horizontal>
						<label>Horizontal</label>
						<displayCond>
							<and>
								<numIndex index="0">USER:T3SBS\T3sbootstrap\UserFunction\TcaMatcher->cardWrapperParent</numIndex>
								<numIndex index="1">FIELD:sDEF.multiImage.enable:=:0</numIndex>
							</and>
						</displayCond>
						<config>
							<type>check</type>
						</config>
					</horizontal>
					<multiImage.enable>
						<label>Enable Profile Card</label>
						<description>You need at least two images for the profile card. The second is the circular, smaller image</description>
						<onChange>reload</onChange>
						<config>
							<type>check</type>
						</config>
					</multiImage.enable>
					<multiImage.percent>
						<label>Width of the circular image (in %)</label>
						<displayCond>FIELD:sDEF.multiImage.enable:=:1</displayCond>
						<config>
							<type>input</type>
							<size>5</size>
							<default>50</default>
							<eval>trim,int</eval>
							<range>
								<lower>10</lower>
								<upper>90</upper>
							</range>
							<slider>
								<step>5</step>
								<width>200</width>
							</slider>
						</config>
					</multiImage.percent>
					<multiImage.borderWidth>
						<label>Border width</label>
						<displayCond>FIELD:sDEF.multiImage.enable:=:1</displayCond>
						<config>
							<type>select</type>
							<renderType>selectSingle</renderType>
							<items type="array">
								<numIndex index="0" type="array">
									<numIndex index="label">none</numIndex>
									<numIndex index="value">none</numIndex>
								</numIndex>
								<numIndex index="1" type="array">
									<numIndex index="label">1</numIndex>
									<numIndex index="value">border border-1</numIndex>
								</numIndex>
								<numIndex index="2" type="array">
									<numIndex index="label">2</numIndex>
									<numIndex index="value">border border-2</numIndex>
								</numIndex>
								<numIndex index="3" type="array">
									<numIndex index="label">3</numIndex>
									<numIndex index="value">border border-3</numIndex>
								</numIndex>
								<numIndex index="4" type="array">
									<numIndex index="label">4</numIndex>
									<numIndex index="value">border border-4</numIndex>
								</numIndex>
								<numIndex index="5" type="array">
									<numIndex index="label">5</numIndex>
									<numIndex index="value">border border-5</numIndex>
								</numIndex>
							</items>
							<default>border border-1</default>
							<size>1</size>
						</config>
					</multiImage.borderWidth>
					<multiImage.borderColor>
						<label>Border color</label>
						<displayCond>FIELD:sDEF.multiImage.enable:=:1</displayCond>
						<config>
							<type>select</type>
							<renderType>selectSingle</renderType>
							<items type="array">
								<numIndex index="0" type="array">
									<numIndex index="label">LLL:EXT:t3sbootstrap/Resources/Private/Language/locallang_be.xlf:secondary</numIndex>
									<numIndex index="value">secondary</numIndex>
								</numIndex>
								<numIndex index="1" type="array">
									<numIndex index="label">LLL:EXT:t3sbootstrap/Resources/Private/Language/locallang_be.xlf:primary</numIndex>
									<numIndex index="value">primary</numIndex>
								</numIndex>
								<numIndex index="2" type="array">
									<numIndex index="label">LLL:EXT:t3sbootstrap/Resources/Private/Language/locallang_be.xlf:success</numIndex>
									<numIndex index="value">success</numIndex>
								</numIndex>
								<numIndex index="3" type="array">
									<numIndex index="label">LLL:EXT:t3sbootstrap/Resources/Private/Language/locallang_be.xlf:info</numIndex>
									<numIndex index="value">info</numIndex>
								</numIndex>
								<numIndex index="4" type="array">
									<numIndex index="label">LLL:EXT:t3sbootstrap/Resources/Private/Language/locallang_be.xlf:warning</numIndex>
									<numIndex index="value">warning</numIndex>
								</numIndex>
								<numIndex index="5" type="array">
									<numIndex index="label">LLL:EXT:t3sbootstrap/Resources/Private/Language/locallang_be.xlf:danger</numIndex>
									<numIndex index="value">danger</numIndex>
								</numIndex>
								<numIndex index="6" type="array">
									<numIndex index="label">light</numIndex>
									<numIndex index="value">light</numIndex>
								</numIndex>
								<numIndex index="7" type="array">
									<numIndex index="label">dark</numIndex>
									<numIndex index="value">dark</numIndex>
								</numIndex>
								<numIndex index="8" type="array">
									<numIndex index="label">white</numIndex>
									<numIndex index="value">white</numIndex>
								</numIndex>
								<numIndex index="9" type="array">
									<numIndex index="label">none</numIndex>
									<numIndex index="value">0</numIndex>
								</numIndex>
							</items>
							<default>white</default>
						</config>
					</multiImage.borderColor>
					<multiImage.shadow>
						<label>Shadow</label>
						<displayCond>FIELD:sDEF.multiImage.enable:=:1</displayCond>
						<config>
							<type>check</type>
							<default>1</default>
						</config>
					</multiImage.shadow>
					<multiImage.diagonal>
						<label>Cut image diagonally</label>
						<displayCond>FIELD:sDEF.multiImage.enable:=:1</displayCond>
						<onChange>reload</onChange>
						<config>
							<type>check</type>
						</config>
					</multiImage.diagonal>
					<multiImage.slope>
						<label>Height of the slope (in %)</label>
						 <displayCond>
							<and>
								<numIndex index="0">FIELD:sDEF.multiImage.enable:=:1</numIndex>
								<numIndex index="1">FIELD:sDEF.multiImage.diagonal:=:1</numIndex>
							</and>
						 </displayCond>
						<config>
							<type>input</type>
							<size>5</size>
							<default>50</default>
							<eval>trim,int</eval>
							<range>
								<lower>10</lower>
								<upper>90</upper>
							</range>
							<slider>
								<step>5</step>
								<width>200</width>
							</slider>
						</config>
					</multiImage.slope>
					<multiImage.socialmedia.enable>
						<label>Add some Social Media Links</label>
						<displayCond>FIELD:sDEF.multiImage.enable:=:1</displayCond>
						<onChange>reload</onChange>
						<config>
							<type>check</type>
						</config>
					</multiImage.socialmedia.enable>
					<multiImage.socialmedia.footer>
						<label>Social Media Links in Card Footer</label>
						<displayCond>
							<and>
								<numIndex index="0">FIELD:sDEF.multiImage.enable:=:1</numIndex>
								<numIndex index="1">FIELD:sDEF.multiImage.socialmedia.enable:=:1</numIndex>
							</and>
						</displayCond>
						<config>
							<type>check</type>
						</config>
					</multiImage.socialmedia.footer>
					<multiImage.socialmedia.facebook>
						<label>Facebook</label>
						<displayCond>
							<and>
								<numIndex index="0">FIELD:sDEF.multiImage.enable:=:1</numIndex>
								<numIndex index="1">FIELD:sDEF.multiImage.socialmedia.enable:=:1</numIndex>
							</and>
						</displayCond>
						<config>
							<type>input</type>
							<renderType>inputLink</renderType>
						</config>
					</multiImage.socialmedia.facebook>
					<multiImage.socialmedia.twitter>
						<label>Twitter</label>
						<displayCond>
							<and>
								<numIndex index="0">FIELD:sDEF.multiImage.enable:=:1</numIndex>
								<numIndex index="1">FIELD:sDEF.multiImage.socialmedia.enable:=:1</numIndex>
							</and>
						</displayCond>
						<config>
							<type>input</type>
							<renderType>inputLink</renderType>
						</config>
					</multiImage.socialmedia.twitter>
					<multiImage.socialmedia.google>
						<label>Google-plus</label>
						<displayCond>
							<and>
								<numIndex index="0">FIELD:sDEF.multiImage.enable:=:1</numIndex>
								<numIndex index="1">FIELD:sDEF.multiImage.socialmedia.enable:=:1</numIndex>
							</and>
						</displayCond>
						<config>
							<type>input</type>
							<renderType>inputLink</renderType>
						</config>
					</multiImage.socialmedia.google>
					<multiImage.socialmedia.linkedin>
						<label>Linkedin</label>
						<displayCond>
							<and>
								<numIndex index="0">FIELD:sDEF.multiImage.enable:=:1</numIndex>
								<numIndex index="1">FIELD:sDEF.multiImage.socialmedia.enable:=:1</numIndex>
							</and>
						</displayCond>
						<config>
							<type>input</type>
							<renderType>inputLink</renderType>
						</config>
					</multiImage.socialmedia.linkedin>
					<multiImage.socialmedia.youtube>
						<label>Youtube</label>
						<displayCond>
							<and>
								<numIndex index="0">FIELD:sDEF.multiImage.enable:=:1</numIndex>
								<numIndex index="1">FIELD:sDEF.multiImage.socialmedia.enable:=:1</numIndex>
							</and>
						</displayCond>
						<config>
							<type>input</type>
							<renderType>inputLink</renderType>
						</config>
					</multiImage.socialmedia.youtube>
					<multiImage.socialmedia.instagram>
						<label>Instagram</label>
						<displayCond>
							<and>
								<numIndex index="0">FIELD:sDEF.multiImage.enable:=:1</numIndex>
								<numIndex index="1">FIELD:sDEF.multiImage.socialmedia.enable:=:1</numIndex>
							</and>
						</displayCond>
						<config>
							<type>input</type>
							<renderType>inputLink</renderType>
						</config>
					</multiImage.socialmedia.instagram>
					<multiImage.socialmedia.pinterest>
						<label>Pinterest</label>
						<displayCond>
							<and>
								<numIndex index="0">FIELD:sDEF.multiImage.enable:=:1</numIndex>
								<numIndex index="1">FIELD:sDEF.multiImage.socialmedia.enable:=:1</numIndex>
							</and>
						</displayCond>
						<config>
							<type>input</type>
							<renderType>inputLink</renderType>
						</config>
					</multiImage.socialmedia.pinterest>
					<multiImage.socialmedia.skype>
						<label>Skype</label>
						<displayCond>
							<and>
								<numIndex index="01">FIELD:sDEF.multiImage.enable:=:1</numIndex>
								<numIndex index="1">FIELD:sDEF.multiImage.socialmedia.enable:=:1</numIndex>
							</and>
						</displayCond>
						<config>
							<type>input</type>
							<renderType>inputLink</renderType>
						</config>
					</multiImage.socialmedia.skype>
				</el>
			</ROOT>
		</sDEF>
		<auto>
			<ROOT>
				<sheetTitle>Auto-layout (if parent wrapper)</sheetTitle>
				<type>array</type>
				<el>
					<gridSystem>
						<label>Auto-layout</label>
						<onChange>reload</onChange>
						<displayCond>USER:T3SBS\T3sbootstrap\UserFunction\TcaMatcher->autoLayoutParent</displayCond>
						<config>
							<type>select</type>
							<renderType>selectSingle</renderType>
							<items type="array">
								<numIndex index="0" type="array">
									<numIndex index="label">no .col</numIndex>
									<numIndex index="value">none</numIndex>
								</numIndex>
								<numIndex index="1" type="array">
									<numIndex index="label">Equal-width</numIndex>
									<numIndex index="value">equal</numIndex>
								</numIndex>
								<numIndex index="2" type="array">
									<numIndex index="label">Column width</numIndex>
									<numIndex index="value">column</numIndex>
								</numIndex>
								<numIndex index="3" type="array">
									<numIndex index="label">Variable width content</numIndex>
									<numIndex index="value">variable</numIndex>
								</numIndex>
							</items>
							<default>equal</default>
							<size>1</size>
						</config>
					</gridSystem>
					<newLine>
						<label>New line (break to a new line for multiple rows after this col)</label>
						<displayCond>USER:T3SBS\T3sbootstrap\UserFunction\TcaMatcher->autoLayoutParent</displayCond>
						<config>
							<type>check</type>
						</config>
					</newLine>
					<alignSelf>
						<label>Align self</label>
						 <displayCond>
								 <or>
								<numIndex index="0">USER:T3SBS\T3sbootstrap\UserFunction\TcaMatcher->autoLayoutParent</numIndex>
								<numIndex index="1">USER:T3SBS\T3sbootstrap\UserFunction\TcaMatcher->flexContainerParent</numIndex>
								 </or>
						 </displayCond>
						<config>
							<type>select</type>
							<renderType>selectSingle</renderType>
							<items type="array">
								<numIndex index="0" type="array">
									<numIndex index="label">none</numIndex>
									<numIndex index="value"></numIndex>
								</numIndex>
								<numIndex index="1" type="array">
									<numIndex index="label">start</numIndex>
									<numIndex index="value">start</numIndex>
								</numIndex>
								<numIndex index="2" type="array">
									<numIndex index="label">end</numIndex>
									<numIndex index="value">end</numIndex>
								</numIndex>
								<numIndex index="3" type="array">
									<numIndex index="label">center</numIndex>
