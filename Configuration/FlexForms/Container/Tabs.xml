<T3DataStructure>
	<meta>
		<langDisable>1</langDisable>
	</meta>
	<sheets>
		<general>
			<ROOT>
				<sheetTitle>LLL:EXT:t3sbootstrap/Resources/Private/Language/locallang_be.xlf:flexforms.settings</sheetTitle>
				<type>array</type>
				<el>
					<hidden>
						<label>Hiding / Display Elements</label>
						<config>
							<type>select</type>
							<renderType>selectSingle</renderType>
							<items type="array">
								<numIndex index="0" type="array">
									<numIndex index="label">none</numIndex>
									<numIndex index="value">0</numIndex>
								</numIndex>
								<numIndex index="1" type="array">
									<numIndex index="label">Hidden only on xs</numIndex>
									<numIndex index="value">d-none d-sm-block</numIndex>
								</numIndex>
								<numIndex index="2" type="array">
									<numIndex index="label">Hidden on sm and down</numIndex>
									<numIndex index="value">d-none d-md-block</numIndex>
								</numIndex>
								<numIndex index="3" type="array">
									<numIndex index="label">Hidden on md and down</numIndex>
									<numIndex index="value">d-none d-lg-block</numIndex>
								</numIndex>
								<numIndex index="4" type="array">
									<numIndex index="label">Hidden on lg and down</numIndex>
									<numIndex index="value">d-none d-xl-block</numIndex>
								</numIndex>
								<numIndex index="5" type="array">
									<numIndex index="label">Hidden only on xl</numIndex>
									<numIndex index="value">d-xl-none</numIndex>
								</numIndex>
								<numIndex index="6" type="array">
									<numIndex index="label">Visible only on xs</numIndex>
									<numIndex index="value">d-block d-sm-none</numIndex>
								</numIndex>
								<numIndex index="7" type="array">
									<numIndex index="label">Visible sm and down</numIndex>
									<numIndex index="value">d-block d-md-none</numIndex>
								</numIndex>
								<numIndex index="8" type="array">
									<numIndex index="label">Visible md and down</numIndex>
									<numIndex index="value">d-block d-lg-none</numIndex>
								</numIndex>
								<numIndex index="9" type="array">
									<numIndex index="label">Visible lg and down</numIndex>
									<numIndex index="value">d-block d-xl-none</numIndex>
								</numIndex>
							</items>
							<default>0</default>
						</config>
					</hidden>

					<display_type>
						<onChange>reload</onChange>
						<label>LLL:EXT:t3sbootstrap/Resources/Private/Language/locallang_be.xlf:flexforms.appearance</label>
						<config type="array">
							<type>select</type>
							<renderType>selectSingle</renderType>
							<items type="array">
								<numIndex index="0" type="array">
									<numIndex index="label">Tabs</numIndex>
									<numIndex index="value">tabs</numIndex>
								</numIndex>
								<numIndex index="1" type="array">
									<numIndex index="label">Pills</numIndex>
									<numIndex index="value">pills</numIndex>
								</numIndex>
								<numIndex index="2" type="array">
									<numIndex index="label">Vertical Pills</numIndex>
									<numIndex index="value">verticalpills</numIndex>
								</numIndex>
							</items>
							<default>tabs</default>
						</config>
					</display_type>
					<aside_width>
						<label>Aside columns width</label>
						<displayCond>FIELD:display_type:=:verticalpills</displayCond>
						<config type="array">
							<type>select</type>
							<renderType>selectSingle</renderType>
							<items type="array">
								<numIndex index="0" type="array">
									<numIndex index="label">1</numIndex>
									<numIndex index="value">1</numIndex>
								</numIndex>
								<numIndex index="1" type="array">
									<numIndex index="label">2</numIndex>
									<numIndex index="value">2</numIndex>
								</numIndex>
								<numIndex index="2" type="array">
									<numIndex index="label">3</numIndex>
									<numIndex index="value">3</numIndex>
								</numIndex>
								<numIndex index="3" type="array">
									<numIndex index="label">4</numIndex>
									<numIndex index="value">4</numIndex>
								</numIndex>
								<numIndex index="4" type="array">
									<numIndex index="label">6</numIndex>
									<numIndex index="value">6</numIndex>
								</numIndex>
							</items>
							<default>3</default>
						</config>
					</aside_width>
					<switch_effect>
						<label>LLL:EXT:t3sbootstrap/Resources/Private/Language/locallang_be.xlf:flexforms.effect</label>
						<config type="array">
							<type>select</type>
							<renderType>selectSingle</renderType>
							<items type="array">
								<numIndex index="0" type="array">
									<numIndex index="label">none</numIndex>
									<numIndex index="value">0</numIndex>
								</numIndex>
								<numIndex index="1" type="array">
									<numIndex index="label">fade</numIndex>
									<numIndex index="value">fade</numIndex>
								</numIndex>
							</items>
							<default>fade</default>
						</config>
					</switch_effect>
					<fill>
						<label>Fill and justify</label>
						<config type="array">
							<type>select</type>
							<renderType>selectSingle</renderType>
							<items type="array">
								<numIndex index="0" type="array">
									<numIndex index="label">none</numIndex>
									<numIndex index="value">0</numIndex>
								</numIndex>
								<numIndex index="1" type="array">
									<numIndex index="label">nav-fill</numIndex>
									<numIndex index="value">nav-fill</numIndex>
								</numIndex>
								<numIndex index="2" type="array">
									<numIndex index="label">nav-justified</numIndex>
									<numIndex index="value">nav-justified</numIndex>
								</numIndex>
							</items>
							<default>0</default>
						</config>
					</fill>
				</el>
			</ROOT>
		</general>
	</sheets>
</T3DataStructure>
