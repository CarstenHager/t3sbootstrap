<T3DataStructure>
	<meta>
		<langDisable>1</langDisable>
	</meta>
	<sheets>
		<general>
			<ROOT>
				<sheetTitle>LLL:EXT:t3sbootstrap/Resources/Private/Language/locallang_be.xlf:flexforms.settings</sheetTitle>
				<type>array</type>
				<el>
					<hidden>
						<label>Hiding / Display Elements</label>
						<config>
							<type>select</type>
							<renderType>selectSingle</renderType>
							<items type="array">
								<numIndex index="0" type="array">
									<numIndex index="label">none</numIndex>
									<numIndex index="value">0</numIndex>
								</numIndex>
								<numIndex index="1" type="array">
									<numIndex index="label">Hidden only on xs</numIndex>
									<numIndex index="value">d-none d-sm-block</numIndex>
								</numIndex>
								<numIndex index="2" type="array">
									<numIndex index="label">Hidden on sm and down</numIndex>
									<numIndex index="value">d-none d-md-block</numIndex>
								</numIndex>
								<numIndex index="3" type="array">
									<numIndex index="label">Hidden on md and down</numIndex>
									<numIndex index="value">d-none d-lg-block</numIndex>
								</numIndex>
								<numIndex index="4" type="array">
									<numIndex index="label">Hidden on lg and down</numIndex>
									<numIndex index="value">d-none d-xl-block</numIndex>
								</numIndex>
								<numIndex index="5" type="array">
									<numIndex index="label">Hidden only on xl</numIndex>
									<numIndex index="value">d-xl-none</numIndex>
								</numIndex>
								<numIndex index="6" type="array">
									<numIndex index="label">Visible only on xs</numIndex>
									<numIndex index="value">d-block d-sm-none</numIndex>
								</numIndex>
								<numIndex index="7" type="array">
									<numIndex index="label">Visible sm and down</numIndex>
									<numIndex index="value">d-block d-md-none</numIndex>
								</numIndex>
								<numIndex index="8" type="array">
									<numIndex index="label">Visible md and down</numIndex>
									<numIndex index="value">d-block d-lg-none</numIndex>
								</numIndex>
								<numIndex index="9" type="array">
									<numIndex index="label">Visible lg and down</numIndex>
									<numIndex index="value">d-block d-xl-none</numIndex>
								</numIndex>
							</items>
							<default>0</default>
						</config>
					</hidden>
					<animation>
						<label>LLL:EXT:t3sbootstrap/Resources/Private/Language/locallang_be.xlf:flexforms.animation</label>
						<config type="array">
							<type>check</type>
							<default>1</default>
						</config>
					</animation>
					<size>
						<label>LLL:EXT:t3sbootstrap/Resources/Private/Language/locallang_be.xlf:flexforms.modalsize</label>
						<config type="array">
							<type>select</type>
							<renderType>selectSingle</renderType>
							<items type="array">
								<numIndex index="0" type="array">
									<numIndex index="label">LLL:EXT:t3sbootstrap/Resources/Private/Language/locallang_be.xlf:default</numIndex>
									<numIndex index="value">default</numIndex></numIndex>
								<numIndex index="1" type="array">
									<numIndex index="label">LLL:EXT:t3sbootstrap/Resources/Private/Language/locallang_be.xlf:small</numIndex>
									<numIndex index="value">modal-sm</numIndex></numIndex>
								<numIndex index="2" type="array">
									<numIndex index="label">LLL:EXT:t3sbootstrap/Resources/Private/Language/locallang_be.xlf:large</numIndex>
									<numIndex index="value">modal-lg</numIndex></numIndex>
								<numIndex index="3" type="array">
										<numIndex index="label">Extra large</numIndex>
										<numIndex index="value">modal-xl</numIndex></numIndex>
								<numIndex index="4" type="array">
									<numIndex index="label">Fullscreen</numIndex>
									<numIndex index="value">modal-fullscreen</numIndex></numIndex>
							</items>
							<default>default</default>
						</config>
					</size>
					<buttonText>
						<label>Link or Button Text (header if empty)</label>
						<config>
							<type>input</type>
							<size>20</size>
							<default></default>
						</config>
					</buttonText>
					<whiteclosebutton>
						<label>White close button</label>
						<config type="array">
							<type>check</type>
							<default>0</default>
						</config>
					</whiteclosebutton>
					<button>
						<label>LLL:EXT:t3sbootstrap/Resources/Private/Language/locallang_be.xlf:flexforms.usebutton</label>
							<onChange>reload</onChange>
						<config type="array">
							<type>check</type>
							<default>1</default>
						</config>
					</button>
					<style>
						<label>LLL:EXT:t3sbootstrap/Resources/Private/Language/locallang_be.xlf:flexforms.buttonstyle</label>
						<displayCond>FIELD:general.button:=:1</displayCond>
						<config type="array">
							<type>select</type>
							<renderType>selectSingle</renderType>
							<items type="array">
								<numIndex index="0" type="array"><numIndex index="label">LLL:EXT:t3sbootstrap/Resources/Private/Language/locallang_be.xlf:default</numIndex>
								<numIndex index="value">default</numIndex></numIndex>
								<numIndex index="1" type="array"><numIndex index="label">LLL:EXT:t3sbootstrap/Resources/Private/Language/locallang_be.xlf:primary</numIndex>
								<numIndex index="value">primary</numIndex></numIndex>
								<numIndex index="2" type="array"><numIndex index="label">LLL:EXT:t3sbootstrap/Resources/Private/Language/locallang_be.xlf:success</numIndex>
								<numIndex index="value">success</numIndex></numIndex>
								<numIndex index="3" type="array"><numIndex index="label">LLL:EXT:t3sbootstrap/Resources/Private/Language/locallang_be.xlf:info</numIndex>
								<numIndex index="value">info</numIndex></numIndex>
								<numIndex index="4" type="array"><numIndex index="label">LLL:EXT:t3sbootstrap/Resources/Private/Language/locallang_be.xlf:warning</numIndex>
								<numIndex index="value">warning</numIndex></numIndex>
								<numIndex index="5" type="array"><numIndex index="label">LLL:EXT:t3sbootstrap/Resources/Private/Language/locallang_be.xlf:danger</numIndex>
								<numIndex index="value">danger</numIndex></numIndex>
							</items>
							<default>default</default>
						</config>
					</style>
					<fixedPosition>
						<label>Fixed position</label>
						<onChange>reload</onChange>
						<displayCond>FIELD:general.button:=:1</displayCond>
						<config>
							<type>select</type>
							<renderType>selectSingle</renderType>
							<items type="array">
								<numIndex index="0" type="array">
									<numIndex index="label">none</numIndex>
									<numIndex index="value">0</numIndex>
								</numIndex>
								<numIndex index="1" type="array">
									<numIndex index="label">Left browser edge</numIndex>
									<numIndex index="value">left</numIndex>
								</numIndex>
								<numIndex index="2" type="array">
									<numIndex index="label">Right browser edge</numIndex>
									<numIndex index="value">right</numIndex>
								</numIndex>
							</items>
							<default>0</default>
						</config>
					</fixedPosition>
					<rotate>
						<label>Rotate the Button</label>
						<displayCond>
							<AND>
								<numIndex index="0">FIELD:general.button:=:1</numIndex>
								<numIndex index="1">FIELD:general.fixedPosition:!=:0</numIndex>
							</AND>
						</displayCond>
						<config>
							<type>select</type>
							<renderType>selectSingle</renderType>
							<items type="array">
								<numIndex index="0" type="array">
									<numIndex index="label">0 degree</numIndex>
									<numIndex index="value">0</numIndex>
								</numIndex>
								<numIndex index="1" type="array">
									<numIndex index="label">+ 90 degree</numIndex>
									<numIndex index="value">plus</numIndex>
								</numIndex>
								<numIndex index="2" type="array">
									<numIndex index="label">- 90 degree</numIndex>
									<numIndex index="value">minus</numIndex>
								</numIndex>
							</items>
							<default>0</default>
						</config>
					</rotate>
				</el>
			</ROOT>
		</general>
	</sheets>
</T3DataStructure>
