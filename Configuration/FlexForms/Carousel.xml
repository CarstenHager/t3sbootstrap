<T3DataStructure>
	<sheets>
		<general>
			<ROOT>
				<type>array</type>
				<TCEforms>
					<sheetTitle>General</sheetTitle>
				</TCEforms>
				<el>
					<shift>
						<TCEforms>
							<label>Vertical shift in % ( 0 = center )</label>
							<displayCond>USER:T3SBS\T3sbootstrap\UserFunction\TcaMatcher->carouselRatioParent</displayCond>
							<config>
								<type>input</type>
								<size>5</size>
								<default>0</default>
								<eval>int</eval>
								<range>
									<lower>0</lower>
									<upper>60</upper>
								</range>
								<wizards>
									<autoplaySpeed>
										<type>slider</type>
										<step>1</step>
										<width>200</width>
									</autoplaySpeed>
								</wizards>
							</config>
						</TCEforms>
					</shift>
				</el>
			</ROOT>
		</general>
	</sheets>
</T3DataStructure>