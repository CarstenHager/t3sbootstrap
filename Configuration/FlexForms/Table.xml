					<tableInverse>
						<TCEforms>
							<label>Table inverse/dark</label>
							<config>
								<type>check</type>
							</config>
						</TCEforms>
					</tableInverse>

					<tableResponsive>
						<TCEforms>
							<label>Responsive table</label>
							<config>
								<type>check</type>
							</config>
						</TCEforms>
					</tableResponsive>

				</el>
			</ROOT>
		</sDEF>
